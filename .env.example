# Your NVIDIA API key is needed to download the NIM container
NVIDIA_API_KEY="YOUR_NVIDIA_API_KEY_HERE"

# The endpoint for the LLM. Defaults to the local NIM service.
NVIDIA_API_URL="http://nim:8000/v1/chat/completions"

# The model to use for generating hallucinations
LLM_MODEL="nvidia/nvidia-nemotron-nano-9b-v2"
